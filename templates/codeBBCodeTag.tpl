<div class="wcf-border wcf-boxTitle wcf-codeBox {$highlighter|get_class|substr:30|lcfirst}">
	<hgroup>
		<h1>{@$highlighter->getTitle()}</h1>
	</hgroup>
	<div>
		<table>
			<tbody>
				<tr>
					<td class="wcf-codeLineNumbers">
						<pre>{@$lineNumbers}</pre>
					</td>
					<td class="wcf-codeLines">
						<pre>{@$content}</pre>
					</td>
				</tr>
			</tbody>
		</table>
	</div>
</div>
